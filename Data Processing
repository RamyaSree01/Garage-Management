##2016 Data Preprocessing
# coding: utf-8

# In[39]:

import pandas as pd
import re
import unicodedata


# In[43]:

df= pd.read_excel('April 2016.xlsx')


# In[44]:

df


# In[45]:

df = df.drop(df.columns[[0, 2, 3, 7, 9, 10, 11, 12, 13]], axis=1)


# In[46]:

df


# In[47]:

df.info()


# In[48]:

df.columns = ['Date', 'Vehicle_No', 'Vehicle_Name', 'Customer_Name', 'Bill_Amount']


# In[49]:

df


# In[50]:

((df.isnull()).sum()/df.shape[0])*100.00


# In[51]:

df = df.dropna(subset=['Vehicle_No','Customer_Name'],how='any')


# In[52]:

((df.isnull()).sum()/df.shape[0])*100.00


# In[53]:

CustName = df.Customer_Name.tolist()
VehicNo = df.Vehicle_No.tolist()
VehicName = df.Vehicle_Name.tolist()
Listoflist = [CustName, VehicNo, VehicName]


# In[54]:

NewCustData = []
NewVehicNoData = []
NewVehicNameData = []
NewListoflist = [NewCustData, NewVehicNoData, NewVehicNameData]
for l in range(len(Listoflist)):
    for i in Listoflist[l]:
        i = str(i)
        data = unicodedata.normalize('NFD'
                                     , i).encode('ascii',
                                                 'ignore')
        data = re.sub('[^a-zA-Z0-9]',' ',
                      data.decode("utf-8"), re.UNICODE)
        data = data.lower()
        NewListoflist[l].append(data)


# In[55]:

df = df.drop(df.columns[[1,2,3]], axis = 1)


# In[56]:

df['Vehicle_No'] = NewVehicNoData
df['Vehicle_Name'] = NewVehicNameData
df['Customer_Name'] = NewCustData


# In[57]:

df.head()


