# In[64]:

((df1.isnull()).sum()/df.shape[0])*100.00


# In[65]:

df1 = df1.dropna(subset=['Vehicle_No','Customer_Name'],how='any')


# In[66]:

df1


# In[67]:

((df1.isnull()).sum()/df.shape[0])*100.00


# In[68]:

CustName = df1.Customer_Name.tolist()
VehicNo = df1.Vehicle_No.tolist()
VehicName = df1.Vehicle_Name.tolist()

#Create a new list which includes the previous extracted lists
Listoflist = [CustName, VehicNo, VehicName]


# In[69]:

NewCustData = []
NewVehicNoData = []
NewVehicNameData = []
NewListoflist = [NewCustData, NewVehicNoData, NewVehicNameData]

for l in range(len(Listoflist)):
    for i in Listoflist[l]:
        i = str(i)
        data = unicodedata.normalize('NFD'
                                     , i).encode('ascii',
                                                 'ignore')
        data = re.sub('[^a-zA-Z0-9]',' ',
                      data.decode("utf-8"), re.UNICODE)
        data = data.lower()
        NewListoflist[l].append(data)


# In[70]:

df1 = df1.drop(df1.columns[[1,2,3]], axis = 1)


# In[71]:

df1['Vehicle_No'] = NewVehicNoData
df1['Vehicle_Name'] = NewVehicNameData
df1['Customer_Name'] = NewCustData


# In[72]:

df1.head()



