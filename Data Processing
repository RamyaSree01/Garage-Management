df3 = pd.read_excel('July2015.xlsx')


# In[59]:

df3


# In[60]:

df3 = df3.drop(df3.columns[[0, 2, 6, 8, 9, 10, 11, 12, 13, 14]], axis=1) 


# In[61]:

df3


# In[62]:

df3.info()


