# In[1]:

import pandas as pd
import re
import unicodedata


# In[2]:

df= pd.read_excel('April2015.xlsx')


# In[3]:

df


# In[4]:

df.columns = df.iloc[0]


# In[5]:

df=df[1:]


# In[6]:

df


# In[7]:

df = df.drop(df.columns[[0]], axis=1) 


# In[8]:

df


# In[9]:

df = df.drop(df.columns[[5, 6, 7, 8, 9, 10, 11]], axis=1)


# In[10]:

df


# In[11]:

df.info()


# In[12]:

df.columns = ['Date', 'Vehicle_No', 'Vehicle_Name', 'Customer_Name', 'Bill_Amount']


# In[13]:

df


# In[14]:

((df.isnull()).sum()/df.shape[0])*100.00


# In[15]:

df = df.dropna(subset=['Vehicle_No','Customer_Name'],how='any')


# In[16]:

df


# In[17]:

((df.isnull()).sum()/df.shape[0])*100.00


# In[18]:

CustName = df.Customer_Name.tolist()
VehicNo = df.Vehicle_No.tolist()
VehicName = df.Vehicle_Name.tolist()
Listoflist = [CustName, VehicNo, VehicName]


# In[19]:

NewCustData = []
NewVehicNoData = []
NewVehicNameData = []
NewListoflist = [NewCustData, NewVehicNoData, NewVehicNameData]
for l in range(len(Listoflist)):
    for i in Listoflist[l]:
        i = str(i)
        data = unicodedata.normalize('NFD'
                                     , i).encode('ascii',
                                                 'ignore')
        data = re.sub('[^a-zA-Z0-9]',' ',
                      data.decode("utf-8"), re.UNICODE)
        data = data.lower()
        NewListoflist[l].append(data)


# In[20]:

df = df.drop(df.columns[[1,2,3]], axis = 1)


# In[21]:

df['Vehicle_No'] = NewVehicNoData
df['Vehicle_Name'] = NewVehicNameData
df['Customer_Name'] = NewCustData


# In[22]:

df.head()


# In[23]:

df


# In[24]:

df.info()


# In[25]:

#MAY DATA PROCESSING STARTS FROM THE BELOW


# In[26]:

df1 = pd.read_excel('2015May.xlsx')


# In[27]:

df1


# In[28]:

df1 = df1.drop(df1.columns[[0, 2, 6, 8, 9, 10, 11, 12, 13]], axis=1) 


# In[29]:

df1


# In[30]:

df1.info()


# In[31]:

df1.columns = ['Date', 'Vehicle_No', 'Vehicle_Name', 'Customer_Name', 'Bill_Amount']


# In[32]:

((df1.isnull()).sum()/df.shape[0])*100.00


# In[33]:

df1 = df1.dropna(subset=['Vehicle_No','Customer_Name'],how='any')


# In[34]:

df1


# In[35]:

((df1.isnull()).sum()/df.shape[0])*100.00


# In[36]:

CustName = df1.Customer_Name.tolist()
VehicNo = df1.Vehicle_No.tolist()
VehicName = df1.Vehicle_Name.tolist()

#Create a new list which includes the previous extracted lists
Listoflist = [CustName, VehicNo, VehicName]


# In[37]:

NewCustData = []
NewVehicNoData = []
NewVehicNameData = []
NewListoflist = [NewCustData, NewVehicNoData, NewVehicNameData]

for l in range(len(Listoflist)):
    for i in Listoflist[l]:
        i = str(i)
        data = unicodedata.normalize('NFD'
                                     , i).encode('ascii',
                                                 'ignore')
        data = re.sub('[^a-zA-Z0-9]',' ',
                      data.decode("utf-8"), re.UNICODE)
        data = data.lower()
        NewListoflist[l].append(data)


# In[38]:

df1 = df1.drop(df1.columns[[1,2,3]], axis = 1)


# In[39]:

df1['Vehicle_No'] = NewVehicNoData
df1['Vehicle_Name'] = NewVehicNameData
df1['Customer_Name'] = NewCustData


# In[40]:

df1.head()


# In[41]:

df1.info()


# In[42]:

#JUNE DATA WILL START FROM BELOW


# In[43]:

df2 = pd.read_excel('June2015.xlsx')


# In[44]:

df2


# In[45]:

df2 = df2.drop(df2.columns[[0, 2, 6, 8, 9, 10, 11, 12, 13]], axis=1) 


# In[46]:

df2


# In[47]:

df2.info()


# In[48]:

df2.columns = ['Date', 'Vehicle_No', 'Vehicle_Name', 'Customer_Name', 'Bill_Amount']


# In[49]:

((df2.isnull()).sum()/df2.shape[0])*100.00


# In[50]:

df2 = df2.dropna(subset=['Vehicle_No','Date'],how='any')


# In[51]:

((df2.isnull()).sum()/df2.shape[0])*100.00


# In[52]:

CustName = df2.Customer_Name.tolist()
VehicNo = df2.Vehicle_No.tolist()
VehicName = df2.Vehicle_Name.tolist()

#Create a new list which includes the previous extracted lists
Listoflist = [CustName, VehicNo, VehicName]


# In[53]:

NewCustData = []
NewVehicNoData = []
NewVehicNameData = []
NewListoflist = [NewCustData, NewVehicNoData, NewVehicNameData]

for l in range(len(Listoflist)):
    for i in Listoflist[l]:
        i = str(i)
        data = unicodedata.normalize('NFD'
                                     , i).encode('ascii',
                                                 'ignore')
        data = re.sub('[^a-zA-Z0-9]',' ',
                      data.decode("utf-8"), re.UNICODE)
        data = data.lower()
        NewListoflist[l].append(data)


# In[54]:

df2 = df2.drop(df2.columns[[1,2,3]], axis = 1)
df2['Vehicle_No'] = NewVehicNoData
df2['Vehicle_Name'] = NewVehicNameData
df2['Customer_Name'] = NewCustData


# In[55]:

df2.head()


# In[56]:

df2.info()


# In[57]:

